{
    "name": "arcanedev/log-viewer",
    "description": "Provides a Log Viewer for Laravel 5",
    "keywords": ["arcanesoft", "laravel", "log", "log viewer", "logviewer"],
    "homepage": "https://github.com/ARCANEDEV/LogViewer",
    "license": "MIT",
    "authors": [
        {
            "name": "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com",
            "homepage": "https://github.com/ARCANEDEV"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "psr/log": "~1.0",
        "arcanedev/support": "~3.3.0"
    },
    "require-dev": {
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "~4.0",
        "phpunit/phpcov": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\LogViewer\\": "src/"
        },
        "files": ["constants.php", "helpers.php"]
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\LogViewer\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit --coverage-text"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.0.x-dev"
        }
    }
}
